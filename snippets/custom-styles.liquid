{% comment %} Trevor's customizations below {% endcomment %}
@media only screen and (min-width: 767px) {
  .featured-collection--root .featured-collection--grid {
    display:flex;
    flex-wrap: wrap;
    justify-content: center;
  }
  .featured-collection--root .featured-collection--grid .product--root {
    flex:0 0 calc(33.333% - 32px);
  }
}
.featured-video--sub-heading, .featured-video--sub-heading-mobile {
    font-family:var(--font--paragraph--family);
    font-weight:normal;
}
.newsletter--root .newsletter--wrapper {
    gap:32px;
}
.newsletter--heading {
	font-size:var(--font--featured-heading--size);
}
.newsletter--subheading {
  	font-size:var(--font--block-heading--size);
  	letter-spacing:.025em;
    line-height: 1.33333em;
    width:100%;
}
.flexbox-gap [data-background-color=dark] .featured-content--text-container {
  gap:32px;
}
.featured-collection--show-more {
  padding-top:32px;
  z-index:1;
}
[data-background-color=dark] .featured-content--link--container, [data-light-text=true] .featured-collection--show-more a {
  background: var(--color--button);
  border-color:var(--bg-color--button);
  border-radius:2px;
  color: var(--bg-color--button);
  letter-spacing: 1px;
  font-size: 14px;
  height: 45px;
  line-height: 45px;
  padding: 0 16px;
  vertical-align: bottom;
  width: auto;
}
[data-background-color=dark] .featured-content--link--container:after, .featured-collection--show-more a:after {
  display:none;
  visibility:hidden;
}
[data-light-text=true] .featured-video--button>* {
  background: var(--color--button);
  border-color:var(--color--button);
  color: var(--bg-color--button);
}
@media only screen and (max-width: 767px) {
  .featured-video--root .featured-video--header-mobile {
    padding:64px 32px;
  }
  .newsletter--heading{font-size:28px;}
  .newsletter--subheading{font-size:18px;}
}
.footer--root .footer--blocks, .footer--root .footer--sub  {
  flex-direction: column;
  align-items: center;
  text-align: center;
}
@media only screen and (min-width: 767px) {
  .footer--menu li {
    display:inline-block;
    letter-spacing:.05em;
    padding:0 8px;
  }
}
.featured-collection--title {    
  display: none!important;
}
@media only screen and (min-width:768px) {
  .product--root .product--label-container[data-item] {
      top: calc(50% - 13.5px);
      left: calc(50% - 61px);
  }
}
@media only screen and (max-width: 767px) {
  .product--root .product--label-container[data-item] {
    top: calc(50% - 42px);
    left: calc(50% - 50px);
  }
}
.product--root .product--label-container[data-item] .product--label {
    color: black;
    padding:6px 15px 5px; 
}
.featured-collection--show-more a {
    background: var(--color--button);
    border-color: var(--bg-color--button);
    border-radius: 2px;
    color: var(--bg-color--button);
    letter-spacing: 1px;
    font-size: 14px;
    height: 45px;
    line-height: 45px;
    padding: 0 16px;
    vertical-align: bottom;
    width: auto;
}
.sold--out .product--image {opacity:.5;}
.product-quantity--input, .product-quantity--input:focus {
	color: var(--color--button);
	padding-right:0;
}
.product-page--root[data-media-spacing=space-evenly] .product-page--grid[data-container=panel][data-columns="2"] {
	align-items:start;
	grid-template-columns: [thumb-img] .125fr [main-img] 1fr;
	grid-auto-flow: dense;
    grid-gap: 48px;
	padding-left:48px;
	padding-top: 48px;
}
.product-page--grid[data-container=panel][data-columns="2"] .product-media--wrapper:first-of-type {
	align-self:center;
	grid-row: 1;
    grid-column: 2;
	border:none;
}
.product-page--grid[data-container=panel][data-columns="2"] .product-media--wrapper {
	grid-column: 1;
    border: 2px solid var(--bg-color--button);
}
.product--root[data-product-view=grid] .product--details-wrapper .product--title,
.product--root[data-product-view=grid] .product--price-wrapper .product--price {
	font-size: 18px;
}

